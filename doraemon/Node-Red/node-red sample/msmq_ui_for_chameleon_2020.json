    {
        "id": "a0a3251.7bf58d8",
        "type": "function",
        "z": "197e9ec8.5806b1",
        "name": "Extract IP",
        "func": "if (msg.statusCode != 200) {\n    return [\n        null,\n        {\n            payload: 'KO ' + msg.statusCode.toString()\n        }\n    ]\n}\n\nlet to_uri = JSON.parse(msg.payload)['pipelines'][0]['to-uri']\nlet regex = /http:\\/\\/([^:/]+):/\n\nlet groups = to_uri.match(regex)\n\nreturn [\n    {\n        payload: groups[1]\n    },\n    {\n        payload: 'OK'\n    }\n]",
        "outputs": 2,
