    {
        "id": "ab69fff9.51872",
        "type": "function",
        "z": "197e9ec8.5806b1",
        "name": "Prepare Request",
        "func": "const CHAMELEON_IP = flow.get('chameleon_ip')\nconst PASSWORD = flow.get('password')\nconst MSMQ_ENTRY_IP = flow.get('msmq_entry_ip')\n\nif (!CHAMELEON_IP || !PASSWORD || !MSMQ_ENTRY_IP) {\n    return [\n        null,\n        {\n            payload: 'KO INPUT'\n        }\n    ]\n}\n\nlet url = 'http://' + CHAMELEON_IP + '/api/v1/safeboxes/etc-mqdclient-config.json'\n\nlet headers = {\n    'Content-Type': 'application/json',\n    'Authorization': 'Basic ' + Buffer.from('admin:' + PASSWORD).toString('base64')\n}\n\nlet payload = {\n    \"queue\": {\n        \"size-per-device\": 4096\n    },\n    \"qurrent\": {\n        \"get-properties-timeout-ms\": 3000,\n        \"subscriber-buffer-size\": 10,\n        \"pk-pairs\": [\n            {\n                \"qurrent-property\": \"machineNo\",\n                \"http-key\": \"machineNo\"\n            },\n            {\n                \"qurrent-property\": \"machineName\",\n                \"http-key\": \"machineName\"\n            }\n        ]\n    },\n    \"http\": {\n        \"post-frequency-ms\": 10,\n        \"max-consumption-per-queue\": 50\n    },\n    \"pipelines\": [\n        {\n            \"from-stream-id\": \"data\",\n            \"to-uri\": \"http://\" + MSMQ_ENTRY_IP + \":12345/mq-diverter/0.1/api/tx8100/Data\"\n        },\n        {\n            \"from-stream-id\": \"alarm_onchange\",\n            \"to-uri\": \"http://\" + MSMQ_ENTRY_IP + \":12345/mq-diverter/0.1/api/tx8100/Event\"\n        }\n    ]\n}\n\nreturn [\n    {\n        url: url,\n        headers: headers,\n        payload: payload\n    },\n    {\n        payload: 'Requesting'\n    }\n]",
        "outputs": 2,
        "noerr": 0,
        "x": 650,
        "y": 440,
        "wires": [
            [
                "e9f2ce60.045758"
            ],
            [
                "255b8a28.56ac16"
            ]
        ]
    },
